<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_group">

    <fragment
        android:id="@+id/navigation_group"
        android:name="com.example.hangyou.ui.group.GroupFragment"
        android:label="@string/title_group"
        tools:layout="@layout/fragment_group" />

    <fragment
        android:id="@+id/navigation_tree_hole"
        android:name="com.example.hangyou.ui.tree_hole.TreeHoleFragment"
        android:label="@string/title_tree_hole"
        tools:layout="@layout/fragment_tree_hole" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.hangyou.ui.home.HomePageFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home_page" />
</navigation>